<Window x:Class="Graficacion.Pacman"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graficacion"
        mc:Ignorable="d"
        Title="Pacman" Height="450" Width="800">
    <Canvas>
        <Ellipse Width="200" Height="200" Stroke="Black" Fill="Yellow"></Ellipse>
        <Path Data="M185,50 135,110 180,160" Stroke="Black" Fill="White"></Path>
        <Ellipse Width="50" Height="150" Canvas.Left="174" Canvas.Top="40" Fill="White"></Ellipse>
        <Ellipse Width="70" Height="70" Fill="White" Canvas.Left="80" Canvas.Top="10"></Ellipse>
        <Ellipse Width="50" Height="50" Fill="Black" Canvas.Left="90" Canvas.Top="20"></Ellipse>
        <Ellipse Width="10" Height="10" Fill="White" Canvas.Left="100" Canvas.Top="30"></Ellipse>
        <Path Data="M160,70 A60,60 0 1 0 160,120 L160,120 120,100 z" Canvas.Left="400" Fill="Yellow" Stroke="Black"></Path>
        <Ellipse Width="40" Height="40" Canvas.Left="500" Canvas.Top="50" Fill="White"></Ellipse>
        <Ellipse Width="30" Height="30" Fill="Black" Canvas.Left="505" Canvas.Top="55"></Ellipse>
        <Ellipse Width="10" Height="10" Fill="White" Canvas.Left="510" Canvas.Top="60"></Ellipse>
    </Canvas>
</Window>
